<div class="container">
  <div class="row">
    <h2 class="text-center">Find Your Future Coach</h2>
      <% @users.each do |user|%>
        <div class="col-xs-12 col-lg-4 coach-card">
            <%= image_tag "https://via.placeholder.com/150", class: "coach-card-avatar" %>
            <h4><%=link_to user.name, user_path(user) %></h4>
            <p><%= user.category %></p>
        </div>
      <% end %>
  </div>
  <div class="row button">
    <a href="/" class="btn-coaches text-center">Go back</a>
  </div>
</div>
